		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"doc.uml": "tsuml --glob ./src/**/*.ts",
		"doc.type": "typedoc --out docs.types src",
		"doc.viz": "tsviz src/ docs.viz/diagram.png",
		"doc.viz.d": "tsviz -d -r src/components docs.viz/diagram_dependencies.png",
		"doc.viz.r": "tsviz -r src/ docs.viz/diagram_recurs.png",
		"doc.tplant": "tplant --input src/components/**/*.ts --output puml/components.puml",
		"doc.tplant.svg": "tplant --input src/components/**/*.ts --output puml/components.svg"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
